    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.8.0",
    "less-loader": "^5.0.0",
    "livereload": "^0.8.2",
    "mini-css-extract-plugin": "^0.4.4",
    "nodemon": "1.19.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "raw-loader": "^0.5.1",
    "style-loader": "^1.1.3",
